<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gotmusic.farfromalaska" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="ios-package" value="UniversalAnalyticsPlugin" />
    </feature>
    <feature name="GameThrivePush">
        <param name="ios-package" value="GameThrivePush" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <access origin="https://oauth.io" />
    <feature name="PushPlugin">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <icon height="180" src="res/ios/appicon-60@3x.png" width="180" />
    <icon height="60" src="res/ios/appicon-60.png" width="60" />
    <icon height="120" src="res/ios/appicon-60@2x.png" width="120" />
    <icon height="76" src="res/ios/appicon-76.png" width="76" />
    <icon height="152" src="res/ios/appicon-76@2x.png" width="152" />
    <icon height="40" src="res/ios/appicon-Small-40.png" width="40" />
    <icon height="80" src="res/ios/appicon-Small-40@2x.png" width="80" />
    <icon height="57" src="res/ios/appicon.png" width="57" />
    <icon height="114" src="res/ios/appicon@2x.png" width="114" />
    <icon height="72" src="res/ios/appicon-72.png" width="72" />
    <icon height="144" src="res/ios/appicon-72@2x.png" width="144" />
    <icon height="29" src="res/ios/appicon-Small.png" width="29" />
    <icon height="58" src="res/ios/appicon-Small@2x.png" width="58" />
    <icon height="50" src="res/ios/appicon-Small-50.png" width="50" />
    <icon height="100" src="res/ios/appicon-Small-50@2x.png" width="100" />
    <splash height="480" src="res/ios/Default.png" width="320" />
    <splash height="960" src="res/ios/Default@2x.png" width="640" />
    <splash height="1024" src="res/ios/Default-Portrait.png" width="768" />
    <splash height="2048" src="res/ios/Default-Portrait@2x.png" width="1536" />
    <splash height="1136" src="res/ios/Default-568h@2x.png" width="640" />
    <splash height="1334" src="res/ios/Default-667h.png" width="750" />
    <splash height="2208" src="res/ios/Default-Portrait-736h.png" width="1242" />
    <name>Far From Alaska</name>
    <description>
        Fique por dentro de todas as novidades da banda Far From Alaska
    </description>
    <author email="contact@gotmusicapp.com" href="http://www.gotmusicapp.com">
        Got Music
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <preference name="ios-orientation-iphone" value="portrait" />
    <preference name="ios-orientation-ipad" value="portrait" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarBackgroundColor" value="#FFFFFF" />
    <preference name="SplashScreen" value="screen" />
</widget>
